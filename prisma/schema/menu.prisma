model Menu {
  id          String      @id @default(uuid())
  name        String
  description String?
  price       Float
  stock       Int         @default(0)
  imageUrl    String?
  categoryId  String
  category    Category    @relation(fields: [categoryId], references: [id])
  orderItems  OrderItem[]
  deletedAt   DateTime? // Untuk Soft Delete
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}
